{
  "name": "starlight-links",
  "version": "0.0.1",
  "license": "MIT",
  "description": "// TODO(HiDeoo)",
  "author": "HiDeoo <github@hideoo.dev> (https://hideoo.dev)",
  "type": "module",
  "main": "./dist/extension.js",
  "scripts": {
    "build": "tsdown",
    "vscode:package": "pnpx @vscode/vsce package",
    "vscode:prepublish": "pnpm build",
    "test": "echo \"Error: no test implemented // TODO(HiDeoo) .\" && exit 1",
    "lint": "eslint . --cache --max-warnings=0 && tsc --noEmit",
    "format": "prettier -w --cache --ignore-unknown .",
    "version": "pnpm changeset version && pnpm i --no-frozen-lockfile"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.7",
    "@hideoo/eslint-config": "^4.0.0",
    "@hideoo/prettier-config": "^2.0.0",
    "@hideoo/tsconfig": "^2.0.1",
    "eslint": "^9.35.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.15.4",
    "typescript": "^5.9.2"
  },
  "engines": {
    "vscode": "^1.102.0"
  },
  "packageManager": "pnpm@10.17.0",
  "private": true,
  "keywords": [
    "// TODO(HiDeoo)"
  ],
  "homepage": "https://github.com/HiDeoo/starlight-links",
  "repository": {
    "type": "git",
    "url": "https://github.com/HiDeoo/starlight-links.git"
  },
  "bugs": "https://github.com/HiDeoo/starlight-links/issues",
  "activationEvents": [
    "onLanguage:plaintext"
  ],
  "contributes": {
    "configuration": {
      "title": "Starlight Links",
      "properties": {
        "starlight-links.configDirectories": {
          "default": [
            ".",
            "./docs"
          ],
          "description": "The directories to look for Starlight configuration files in (relative to the workspace root).",
          "type": "array"
        }
      }
    }
  },
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "extensionKind": [
    "workspace"
  ],
  "prettier": "@hideoo/prettier-config"
}
